<h1>Create a Team and Heroes!</h1>
<form action="/teams" method="post">
  <label for="team[name]">Team name:</label><input type="text" name="team[name]" id="team[name]"><br>
  <label for="team[motto]">Team motto:</label><input type="text" name="team[motto]" id="team[motto]"><br>
  <label for="member1_name">Member 1 name:</label><input type="text" name="team[members][][name]" id="member1_name"><br>
  <label for="member1_power">Member 1 power:</label><input type="text" name="team[members][][power]" id="member1_power"><br>
  <label for="member1_bio">Member 1 biography:</label><input type="text" name="team[members][][bio]" id="member1_bio"><br>
  <label for="member2_name">Member 2 name:</label><input type="text" name="team[members][][name]" id="member2_name"><br>
  <label for="member2_power">Member 2 power:</label><input type="text" name="team[members][][power]" id="member2_power"><br>
  <label for="member2_bio">Member 2 biography:</label><input type="text" name="team[members][][bio]" id="member2_bio"><br>
  <label for="member3_name">Member 3 name:</label><input type="text" name="team[members][][name]" id="member3_name"><br>
  <label for="member3_power">Member 3 power:</label><input type="text" name="team[members][][power]" id="member3_power"><br>
  <label for="member3_bio">Member 3 biography:</label><input type="text" name="team[members][][bio]" id="member3_bio"><br>
  <input type="submit" id="submit">
</form>
